{
  "name": "native-variants",
  "version": "0.1.15",
  "description": "A library for handling variants in React Native components with theme support.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@9.12.3",
  "scripts": {
    "build": "swc packages -d dist --copy-files --strip-leading-paths --delete-dir-on-start --ignore */.{spec,test}.ts && tsc && tsc-alias --project tsconfig.build.json",
    "lint": "eslint packages/**/*.ts",
    "prepare": "husky",
    "deploy": "npm version patch && npm publish --access public"
  },
  "engines": {
    "node": "^20"
  },
  "keywords": [
    "slots",
    "variants",
    "style-sheet",
    "react-native",
    "coumpound-variants"
  ],
  "author": "matheusscode <matheussdev3@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@swc/cli": "^0.5.2",
    "@swc/core": "^1.10.1",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.0"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-native": "^0.76.5",
    "husky": "^9.1.7"
  }
}
